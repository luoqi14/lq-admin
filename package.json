{
  "name": "react-admin-pro",
  "version": "1.0.0",
  "description": "react16 + redux + router4 + antd3",
  "main": "index.js",
  "engines": {
    "node": ">=8.5.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "clean": "rimraf build",
    "clean:map": "rimraf build/*.map",
    "compile": "cross-env npm run clean && DEBUG=app:* node bin/compile",
    "compile:dev": "cross-env NODE_ENV=dev npm run compile",
    "compile:qafc": "cross-env NODE_ENV=qafc npm run compile",
    "compile:pre": "cross-env NODE_ENV=pre npm run compile",
    "compile:online": "cross-env NODE_ENV=online npm run compile",
    "lint": "eslint --ext .jsx,.js src/",
    "lint:fix": "npm run lint -- --fix",
    "start": "cross-env DEBUG=app:* NODE_ENV=local node bin/dev-server",
    "start:mock": "cross-env MOCK=true npm start",
    "dev": "cross-env DEBUG=app:* NODE_ENV=local nodemon bin/dev-server --ignore build --ignore coverage --ignore tests --ignore src",
    "dll": "webpack --config config/webpack.dll.config.js --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luoqi14/lq-admin.git"
  },
  "author": "luoq@xinguangnet.com",
  "license": "MIT",
  "dependencies": {
    "antd": "^3.13.6",
    "babel-polyfill": "^6.26.0",
    "history": "^4.7.2",
    "moment": "^2.19.1",
    "react": "^16.4.1",
    "react-custom-scrollbars": "^4.2.0",
    "react-dom": "^16.3.1",
    "react-quill": "^1.2.7",
    "react-redux": "^5.0.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redbox-react": "^1.2.10",
    "redux": "^3.7.2",
    "redux-thunk": "^2.0.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@epegzz/sass-vars-loader": "^2.0.2",
    "add-asset-html-webpack-plugin": "^2.1.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-imports-transform": "^0.1.0",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cross-env": "^5.1.1",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.7",
    "cssnano": "^3.7.4",
    "debug": "^2.6.9",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.4.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^1.0.0",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "ip": "^1.1.2",
    "less": "^2.7.3",
    "less-loader": "^3.0.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.8.3",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.1.0",
    "postcss-pxtorem": "^3.3.1",
    "prop-types": "^15.6.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "svg-sprite-loader": "^0.3.1",
    "url-loader": "^0.5.8",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-hot-middleware": "^2.21.2",
    "yargs": "^11.0.0"
  }
}
