{
  "name": "lq-admin",
  "version": "1.0.0",
  "description": "react16 + redux + router4 + antd3",
  "main": "index.js",
  "engines": {
    "node": ">=8.5.0 <12.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "clean": "rimraf build",
    "compile": "cross-env npm run clean && node bin/compile",
    "compile:dev": "cross-env NODE_ENV=dev npm run compile",
    "compile:qaif": "cross-env NODE_ENV=qaif npm run compile",
    "compile:qafc": "cross-env NODE_ENV=qafc npm run compile",
    "compile:pre": "cross-env NODE_ENV=pre npm run compile",
    "compile:online": "cross-env NODE_ENV=online npm run compile",
    "lint": "eslint --ext .jsx,.js src/",
    "lint:fix": "npm run lint -- --fix",
    "start": "cross-env DEBUG=app:* NODE_ENV=local node bin/dev-server",
    "start:mock": "cross-env MOCK=true npm start",
    "dll": "webpack --config config/webpack.dll.config.js --mode development",
    "release": "standard-version -r minor && git push origin master --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+http://git.caicaivip.com:frontend/dailuobo-admin-web.git"
  },
  "author": "luoq@xinguangnet.com",
  "license": "MIT",
